data{\n    int<lower=1> N;\n    real z[N];\n    int x[N];\n    int y[N];\n}\nparameters{\n    real A;\n    real B;\n    real C;\n    real D;\n    real E;\n    real G;\n    real<lower=0> sigma;\n}\nmodel{\n    vector[N] mu;\n    sigma ~ cauchy( 0 , 2 );\n    G ~ normal( 0 , 10 );\n    E ~ normal( 0 , 10 );\n    D ~ normal( 0 , 10 );\n    C ~ normal( 0 , 10 );\n    B ~ normal( 0 , 10 );\n    A ~ normal( 0 , 10 );\n    for ( i in 1:N ) {\n        mu[i] = A * ((x[i])^2) + B * ((y[i])^2) + C * (x[i] * y[i]) + D * (x[i]) + E * (y[i]) +      G;\n    }\n    z ~ normal( mu , sigma );\n}\ngenerated quantities{\n    vector[N] mu;\n    real dev;\n    dev = 0;\n    for ( i in 1:N ) {\n        mu[i] = A * ((x[i])^2) + B * ((y[i])^2) + C * (x[i] * y[i]) + D * (x[i]) + E * (y[i]) +      G;\n    }\n    dev = dev + (-2)*normal_lpdf( z | mu , sigma );\n}\n
